\name{sparsifyDynamics}
\alias{sparsifyDynamics}
\title{Add together two numbers.}
\usage{
sparsifyDynamics(Theta, dXdt, lambda, n)
}
\arguments{
\item{x}{A number.}

\item{y}{A number.}
}
\value{
The sum of \code{x} and \code{y}.
}
\description{
Add together two numbers.
}
\examples{
add(1, 1)
add(10, 1)
}
